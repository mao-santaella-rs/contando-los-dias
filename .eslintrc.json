{
  "extends": ["plugin:vue/recommended", "@vue/prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "vue/no-v-html": "off",
    "no-unused-vars": "off",
    "no-console": "off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "globals": {
    "describe": "readonly",
    "beforeEach": "readonly",
    "it": "readonly",
    "expect": "readonly",
    "jest": "readonly",
    "import": "readonly",
    "google": "readonly",
    "rewardStyle": "readonly"
  },
  "overrides": [
    {
      "files": ["**/*.spec.js", "**/__tests__/*.{j,t}s?(x)"],
      "rules": {
        "no-unused-expressions": "off",
        "no-unused-vars": "off",
        "no-console": "off"
      }
    }
  ],
  "env": {
    "browser": true,
    "amd": true,
    "node": true
  }
}
